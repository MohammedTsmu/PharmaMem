<Window x:Class="PharmaMem.ManageImagesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Manage Images" Height="450" Width="400">
    <Grid>
        <StackPanel Margin="10">
            <ListBox Name="ImagesList" Height="300"/>
            <Button Content="Add Image" Click="AddImage_Click"/>
            <Button Content="Delete Image" Click="DeleteImage_Click"/>
        </StackPanel>
    </Grid>
</Window>
